{
  "version": "1.1.2-0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@iii8iii/thsbot",
  "author": "iii8iii",
  "module": "dist/thsbot.esm.js",
  "size-limit": [
    {
      "path": "dist/thsbot.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/thsbot.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.5.5",
    "husky": "^7.0.1",
    "playwright-firefox": "^1.13.1",
    "size-limit": "^5.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@iii8iii/wechatbot": "^1.0.0",
    "@types/lodash": "^4.14.172",
    "jimp": "^0.16.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  }
}
